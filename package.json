    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sass": "^1.2.12",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "1.51.0",
    "sass-loader": "^12.6.0",
    "svgo": "^2.8.0",
    "testcafe": "1.9.2",
    "webpack": "^5.72.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axa-ch-webhub-cloud/pattern-library.git"
  },
