{
  "name": "@axa-ch/pattern-library",
  "version": "2.0.0",
  "description": "[Check out our Contribution File](https://github.com/axa-ch-webhub-cloud/pattern-library/blob/develop/CONTRIBUTION.md#rules-and-lintings)",
  "module": "lib",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build-components",
    "watch": "npm run watch-components",
    "build-components": "lerna run build --parallel",
    "watch-components": "lerna run watch --parallel",
    "setup-npm-links": "node scripts/setup-npm-link.js",
    "postinstall": "lerna bootstrap --hoist && cross-env NODE_ENV=production npm run build-components && lerna run --parallel genPackagelock",
    "release": "git clean -dfxq -e .idea -e .vscode && npm whoami && npx lerna clean && npm ci && lerna version --exact && cross-env NODE_ENV=production npm run build-components && lerna publish from-package && git clean -dfxq -e .idea -e .vscode && npm install && git add -A && git commit -m \"release: update package locks\" && git push",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 --no-version-updates",
    "build-storybook": "build-storybook",
    "test": "npm run test-lint && prettier --check . && node ./scripts/build.test.js && npm run test-ui",
    "test-lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "test-jest": "jest --env=jsdom",
    "test-playwright": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 jest --config=jest.ui.config.js",
    "debug-playwright": "PWDEBUG=1 TEST_HOST_STORYBOOK_URL=http://localhost:6006 jest --config=jest.ui.config.debug.js",
    "test-ui": "sh scripts/ui-test-runner.sh",
    "test-testcafe": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 testcafe chrome ./**/ui.test.js",
    "test-testcafe-headless": "TEST_HOST_STORYBOOK_URL=http://localhost:6006 testcafe 'chrome:headless' ./**/ui.test.js",
    "new": "node scripts/create-a-m-o/index.js",
    "deploy-branch": "npm run build-storybook && SOURCE_BRANCH_NAME=\"$(git rev-parse --abbrev-ref HEAD)\" bash scripts/branch-deployment.sh && SOURCE_BRANCH_NAME=\"$(git rev-parse --abbrev-ref HEAD)\" bash scripts/branch-deployment-notifier.sh",
    "link-components": "sh scripts/npm-link-components.sh",
    "check-for-uncommitted-files": "git status && git diff-index --quiet HEAD --"
  },
  "dependencies": {
    "@skatejs/val": "^0.4.0",
    "@ungap/global-this": "^0.4.4",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "lit": "^2.2.1"
  },
  "devDependencies": {
    "@axa-ch/materials": "latest",
    "@babel/core": "^7.3.3",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@storybook/addon-a11y": "6.5.5",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-viewport": "6.5.5",
    "@storybook/addons": "6.5.5",
    "@storybook/builder-webpack5": "6.5.5",
    "@storybook/components": "6.5.5",
    "@storybook/manager-webpack5": "6.5.5",
    "@storybook/theming": "6.5.5",
    "@storybook/web-components": "6.5.5",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.0.5",
    "chalk": "^4.1.2",
    "clipboardy": "^2.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^2.6.12",
    "cp": "^0.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-extended": "^2.9.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "github-markdown-css": "^4.0.0",
    "glob": "^8.0.1",
    "jest": "^27.5.1",
    "jest-playwright-preset": "1.7.2",
    "lerna": "^4.0.0",
    "marked": "^1.2.4",
    "mkdirp": "^0.5.1",
    "mutationobserver-shim": "^0.3.3",
    "mv": "^2.1.1",
    "outdent": "^0.7.0",
    "playwright-chromium": "^1.22.1",
    "postcss": "^7.0.26",
    "prettier": "2.6.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
